{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 300,
            "excludeFiles": "{tests/**,**/__pycache__/**,site/**,docs/**,*.csv,*.md,scripts/**}"
        }
    }
}
